@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix modgui: <http://moddevices.com/ns/modgui#>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<http://VeJaPlugins.com/plugins/Release/m8vca> 
a lv2:Plugin, mod:ControlVoltagePlugin;
lv2:requiredFeature urid:map ;

doap:name "MARS8 VCA";

doap:developer [
    foaf:name "Jarno & Jan";
    foaf:homepage <>;
    foaf:mbox <mailto:jan@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "Veja Plugins";
    foaf:homepage <>;
    foaf:mbox <mailto:jan@moddevices.com>;
    ];

    lv2:minorVersion 2;
    lv2:microVersion 1;

rdfs:comment """
CV module that contains the VCA used inside the Mars-8 project 
""";

lv2:port
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 0;
    lv2:symbol "INPUT";
    lv2:name "Input";
]
,
[
    a lv2:InputPort, lv2:CVPort, mod:CVPort;
    lv2:index 1;
    lv2:symbol "INP_2";
    lv2:name "CV Env Input";
]
,
[
    a lv2:InputPort, lv2:CVPort, mod:CVPort;
    lv2:index 2;
    lv2:symbol "INP_1";
    lv2:name "CV LFO Input";
]
,
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 3;
    lv2:symbol "OUTPUT";
    lv2:name "Output";
]
,
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "INP_1_MOD";
    lv2:name "LFO modulation";
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 10.0 ;
]
,
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 5;
    lv2:symbol "LEVEL";
    lv2:name "Level";
    lv2:default 5.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 10.0 ;
]
.
